string = input()
string_need = input()

if string_need in string:
    b = len(string)
    a = len(string_need)
    x = (3.14 + b) / a
    print(x)
else:
    if len(string) % 2 == 0 and len(string) % 5 == 0:
        print(string + string_need)
        print(len(string + string_need))
    elif len(string) % 2 == 1 and len(string) % 3 == 0:
        a = input()
        b = input()
        c = input()
        if len(a) >= len(b) and len(a) >= len(c):
            print(a)
        if len(b) >= len(a) and len(b) >= len(c):
            print(b)
        if len(c) >= len(b) and len(c) >= len(c):
            print(c)
    else:
        print('Exit')
